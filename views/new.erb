<h2>Add a band</h2>

<form action="/bands" method="post">
  <label>
    Name:
    <input type="text"  name="name">
  </label>
  <label>
    Year Formed:
    <input type="text"  name="formed">
  </label>
  <label>
    Together:
    <input type="text"  name="together">
  </label>
  <label>
    Photo:
    <input type="text"  name="photo">
  </label>

  <input type="submit" value="Add band">
</form>
